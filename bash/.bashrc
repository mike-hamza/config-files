alias ll='ls -lh'
alias la='ls -alhF'
alias l='ls -AhF'
alias df='df -h'
alias du='du -h'
alias free='free -m'
alias grep='grep --color=auto'
alias egrep='egrep --color=auto'
alias fgrep='fgrep --color=auto'
alias cdd='cd -' 
alias ..='cd ..'
alias ...='cd ../..'
alias ....='cd ../../..'
